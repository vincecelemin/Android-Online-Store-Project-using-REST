<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.concept">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_concept"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_concept"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpCustomer"
            android:theme="@style/AppThemeNav" />
        <activity
            android:name=".ProductView"
            android:label="@string/title_activity_product_view"
            android:theme="@style/AppThemeNav" />
        <activity
            android:name=".UpdateProfile"
            android:theme="@style/AppThemeNav" />
        <activity
            android:name=".ChangePassword"
            android:theme="@style/AppThemeNav" />
        <activity android:name=".ViewProduct" />
        <activity
            android:name=".TopUp"
            android:theme="@style/AppThemeNav" />
        <activity
            android:name=".CheckOut"
            android:theme="@style/AppThemeNav" />
        <activity
            android:name=".EditShippingDetails"
            android:theme="@style/AppThemeNav" />
        <activity
            android:name=".PaymentSelection"
            android:theme="@style/AppThemeNav" />
        <activity
            android:name=".LoadTransactions"
            android:theme="@style/AppThemeNav" />
        <activity
            android:name=".Orders"
            android:theme="@style/AppThemeNav" />
    </application>

</manifest>